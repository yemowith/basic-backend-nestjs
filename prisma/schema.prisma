generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Logs {
  id        String   @id @default(uuid())
  level     String
  message   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  metadata  Json
  trace     String?
  stack     String?
  context   String?
  request   String?
  response  String?
  error     String?
}
